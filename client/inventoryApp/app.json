{
  "expo": {
    "name": "SIGRAPI",    
    "slug": "inventoryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f711f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.rangel3l.inventoryApp"
    },
    "web": {
      "output": "static",
      "favicon": ". src/assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",     
      "expo-font"
       
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da0eece5-aaae-4a13-b347-81b981d6e8d3"
      }
    }
  }
}
